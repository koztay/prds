
# PostgreSQL
POSTGRES_PASSWORD=<YOUR_POSTGRES_PASSWORD>
POSTGRES_USER=pureads

DATABASE_URL=postgres://pureads:<YOUR_POSTGRES_PASSWORD>@127.0.0.1:5432//pureads

# General settings
# DJANGO_READ_DOT_ENV_FILE=True
DJANGO_ADMIN_URL=<YOUR_DJANGO_ADMIN_URL>
DJANGO_SETTINGS_MODULE=config.settings.production
DJANGO_SECRET_KEY=<YOUR_DJANGO_SECRET_KEY>
DJANGO_ALLOWED_HOSTS=.pureads.com


# AWS Settings
DJANGO_AWS_ACCESS_KEY_ID=
DJANGO_AWS_SECRET_ACCESS_KEY=
DJANGO_AWS_STORAGE_BUCKET_NAME=


# Used with email
DJANGO_MAILGUN_API_KEY=<YOUR_DJANGO_MAILGUN_API_KEY>
DJANGO_SERVER_EMAIL=<YOUR_DJANGO_SERVER_EMAIL>
MAILGUN_SENDER_DOMAIN=<MAILGUN_SENDER_DOMAIN>

# Security! Better to use DNS for this task, but you can use redirect
DJANGO_SECURE_SSL_REDIRECT=False

# django-allauth
DJANGO_ACCOUNT_ALLOW_REGISTRATION=True


COMPRESS_ENABLED=True
